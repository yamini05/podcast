<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_discover">

    <fragment
        android:id="@+id/navigation_discover"
        android:name="com.example.podcast.ui.discover.DiscoverFragment"
        android:label="@string/title_discover"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.podcast.ui.community.CommunityFragment"
        android:label="@string/community"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_library"
        android:name="com.example.podcast.ui.library.LibraryFragment"
        android:label="@string/library" />
    <fragment
        android:id="@+id/navigation_personal"
        android:name="com.example.podcast.ui.library.LibraryFragment"
        android:label="@string/Personal"
        tools:layout="@layout/fragment_notifications" />
</navigation>